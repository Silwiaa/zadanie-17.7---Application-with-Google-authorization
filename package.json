{
  "name": "Webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-webp-d": "webpack-dev-server --hot --env development",
    "start-webp-p": "webpack --mode production",
    "webpack-server": "npm install --save-dev webpack-dev-server",
    "start-node": "node src/app.js",
    "start": "http-server",
    "react-hot-loader": "npm install --save-dev react-hot-loader",
    "instal-babel": "npm install --save-dev babel-cli babel-preset-latest",
    "instal-google_auth_set": "npm install express passport passport-google-oauth",
    "instal-pug": "npm install --save pug",
    "instal-express": "npm install express --save",
    "instal-body-parsel": "npm install body-parser --save",
    "babel-compile": "babel script.js --watch --out-file script-compiled.js",
    "loader-babel": "npm install --save-dev babel-loader babel-core",
    "loader-css": "npm install --save-dev style-loader css-loader",
    "preset-env": "npm install babel-preset-env --save-dev",
    "preset-jsx": "npm install babel-preset-react --save-dev",
    "plugin-template": "npm install html-webpack-plugin --save-dev",
    "plugin-optimize-js": "npm install --save-dev optimize-js-plugin",
    "react": "npm install --save-dev react-dom react",
    "uuid": "npm install --save uuid",
    "test": "npm-run-all test:*",
    "test:html": "node_modules/.bin/globstar nu-html-checker *.html",
    "test:js": "jshint script.js",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdir sass css vendor images src build  src/containers src/components",
    "init:files": "touch src/index.html sass/style.scss script.js .babelrc src/index.js  src/container/App.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "node_modules/.bin/onchange -d 100 sass/style.scss -- node-sass --output-style expanded --source-map true -o css sass && autoprefixer-cli -o css/style.prefixed.css css/style.css",
    "watch:browsersync": "browser-sync start --server --files *.html *.css *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silwiaa/Git-Task-4.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Silwiaa/Git-Task-4/issues"
  },
  "homepage": "https://github.com/Silwiaa/Git-Task-4#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.6",
    "css-loader": "^1.0.0",
    "globstar": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.9.5",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0",
    "optimize-js-plugin": "0.0.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pug": "^2.0.3",
    "uuid": "^3.3.2"
  }
}
